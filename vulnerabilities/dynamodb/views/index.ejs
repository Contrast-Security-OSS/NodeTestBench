 <% include ../../../views/partials/header %>


		 <h1 class="page-header">DynamoDB (XSS)</h1>

		  <div class="row">
		  	<div class="col-xs-12 col-sm-6" style="padding-bottom: 30px;">
				<h4 class="sub-header">DynamoDB Document Client</h4>
				<form method="POST" action="/ddb/docGetItem" target="_blank">
				  <div class="form-group">
				    <label for="exampleInputEmail1">get</label>
				    <input name="id" class="form-control" id="exampleInputEmail1" placeholder="Lookup ID" value="">
				  </div>
				  <button type="submit" class="btn btn-default">Submit</button>
				</form>
				<form method="POST" action="/ddb/docPutItem" target="_blank">
				  <div class="form-group">
				    <label for="exampleInputEmail1">put</label>
				    <input name="id" class="form-control" id="exampleInputEmail2" placeholder="ID" value="">
				    <input name="key" class="form-control" id="exampleInputEmail3" placeholder="Attr 1" value="">
				    <input name="key2" class="form-control" id="exampleInputEmail4" placeholder="Attr 2" value="">
				  </div>
				  <button type="submit" class="btn btn-default">Submit</button>
				</form>
			</div>
		  <div class="row">
		  	<div class="col-xs-12 col-sm-6" style="padding-bottom: 30px;">
				<h4 class="sub-header">DynamoDB Client</h4>
				<form method="POST" action="/ddb/getItem" target="_blank">
				  <div class="form-group">
				    <label for="exampleInputEmail1">getItem()</label>
				    <input name="input" class="form-control" id="exampleInputEmail1" placeholder="" value="woot">
				  </div>
				  <button type="submit" class="btn btn-default">Submit</button>
